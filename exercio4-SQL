1. SELECT COUNT(id) FROM imoveis WHERE cidade = 'São Paulo' AND andar = '3' AND total <= '200000' OR cidade = 'São Paulo' AND andar = '4' AND total <= '200000';

2. SELECT AVG(aluguel) FROM imoveis WHERE cidade = 'São Paulo' AND andar = '3' AND total <= '200000' OR cidade = 'São Paulo' AND andar = '4' AND total <= '200000';

SELECT AVG(condominio) FROM imoveis WHERE cidade = 'São Paulo' AND andar = '3' AND total <= '200000' OR cidade = 'São Paulo' AND andar = '4' AND total <= '200000';

3. SELECT COUNT(id) FROM imoveis WHERE 
cidade = 'São Paulo' AND andar = '3' AND total <= 200000 OR 
cidade = 'São Paulo' AND andar = '4' AND total <= 200000 OR
cidade = 'Rio de Janeiro' AND andar = '3' AND total <= 200000 OR
cidade = 'Rio de Janeiro' AND andar = '4' AND total <= 200000 OR
cidade = 'Belo Horizonte' AND andar = '3' AND total <= 200000 OR
cidade = 'Belo Horizonte' AND andar = '4' AND total <= 200000;

4. SELECT AVG(aluguel) FROM imoveis WHERE 
