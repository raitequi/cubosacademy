cidade = 'Belo Horizonte' AND andar = '4' AND total <= 200000;

5. SELECT COUNT(cidade) FROM imoveis WHERE cidade = 'São Paulo' AND andar <> '-';

6. SELECT COUNT(cidade) FROM imoveis WHERE cidade = 'São Paulo' AND andar = '-';

7. SELECT COUNT(cidade) FROM imoveis WHERE cidade = 'São Paulo';
SELECT COUNT(cidade) FROM imoveis WHERE cidade = 'Campinas';
SELECT COUNT(cidade) FROM imoveis WHERE cidade = 'Belo Horizonte';
SELECT COUNT(cidade) FROM imoveis WHERE cidade = 'Rio de Janeiro';
SELECT COUNT(cidade) FROM imoveis WHERE cidade = 'Porto Alegre';

8. SELECT COUNT(cidade) FROM imoveis WHERE cidade = 'São Paulo' AND banheiros >=2 AND animais = TRUE;
SELECT COUNT(cidade) FROM imoveis WHERE cidade = 'Campinas' AND banheiros >=2 AND animais = TRUE;
SELECT COUNT(cidade) FROM imoveis WHERE cidade = 'Belo Horizonte' AND banheiros >=2 AND animais = TRUE;
SELECT COUNT(cidade) FROM imoveis WHERE cidade = 'Rio de Janeiro' AND banheiros >=2 AND animais = TRUE;
SELECT COUNT(cidade) FROM imoveis WHERE cidade = 'Porto Alegre' AND banheiros >=2 AND animais = TRUE;

9. SELECT AVG(aluguel) FROM imoveis WHERE cidade = 'São Paulo' AND banheiros >=2 AND animais = TRUE;
SELECT AVG(aluguel) FROM imoveis WHERE cidade = 'Campinas' AND banheiros >=2 AND animais = TRUE;
